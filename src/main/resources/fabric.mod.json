{
  "schemaVersion": 1,
  "id": "vitra",
  "version": "${version}",
  "name": "Vitra",
  "description": "High-performance optimization and multi-backend rendering mod for Minecraft",
  "authors": [
    "CriticalRange"
  ],
  "contact": {
    "homepage": "https://github.com/CriticalRange/Vitra",
    "sources": "https://github.com/CriticalRange/Vitra"
  },
  "license": "MIT",
  "icon": "assets/vitra/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.vitra.fabric.VitraModFabric"
    ],
    "client": [
      "com.vitra.fabric.client.VitraModFabricClient"
    ]
  },
  "mixins": [
    "vitra.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.3",
    "minecraft": ">=26.1-alpha.1",
    "java": ">=25",
    "fabric-api": "*"
  },
  "custom": {
    "fabric-renderer-api-v1:contains_renderer": true
  }
}